{% extends 'front.html.twig' %}
{% block body %}
{{ form_start(form) }}

<div class="form-row align-items-end" >
    <div class="col">
        {% if form.type is defined %}
        {{ form_row(form.type) }}

    </div>
    {% endif %}

    <div class="col">
        <div class="form-group">
            <button type="submit" class="btn btn-success">Rechercher</button>
        </div>
    </div>
</div>
{{ form_end(form) }}
{% endblock %}